<?xml version="1.0" encoding="utf-8"?>
<FileManager>
	<mainFolder>/</mainFolder>
	<uploadFileMaxSize>5</uploadFileMaxSize> <!-- in MegaByte -->
	<colorScheme>
		<directory>
			<background>#eeae4a</background>
			<color>white</color>
		</directory>
		<application>
			<background>#16a085</background>
			<color>white</color>
		</application>
		<text>
			<background>#bdc3c7</background>
			<color>#2c3e50</color>
		</text>
		<audio>
			<background>#3498db</background>
			<color>white</color>
		</audio>
		<video>
			<background>#9b59b6</background>
			<color>white</color>
		</video>
		<image>
			<background>#27ae60</background>
			<color>white</color>
		</image>
	</colorScheme>
</FileManager>